<div class="form">
  <%= form_for(:place, url: places_path) do |f| %>
    <h2 class="text-center">Propose Place</h2>   
    <div class="form-group">
      <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-user"></i></span>
          <%= f.text_field :name, autofocus: true, class: "form-control", required: true, placeholder:"Name" %>    
        </div>
    </div>
    <div class="form-group">
      <div class="input-group">
        <span class="input-group-addon"><i class="fa fa-lock"></i></span>    
        <%= f.text_area :address, autofocus: true, autocomplete: "off", class: "form-control", required: true, placeholder:"Address" %>    
      </div>
    </div>        
    <div class="form-group">
      <%= f.submit "Propose Place", :class => "btn btn-secondary login-btn btn-block" %>
    </div>
  <% end %>
</div>